{
    "name": "dfdb-client",
    "version": "0.0.11",
    "description": "A simple terminal to interact with DocsOnFileDB databases.",
    "main": "cmd.js",
    "preferGlobal": true,
    "bin": "./cmd.js",
    "scripts": {
        "start": "node cmd.js",
        "gen:docs": "node .tools/gen-docs.js",
        "pretest": "rm -fr ./test/tmp/*",
        "test": "mkdir -p ./test/tmp;./node_modules/mocha/bin/mocha"
    },
    "keywords": [
        "nosql",
        "emulated",
        "serverless",
        "on-file",
        "zip"
    ],
    "author": {
        "name": "Alejandro Dario Simi",
        "url": "http://daemonraco.com/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/daemonraco/dfdb-client.git"
    },
    "license": "MIT",
    "dependencies": {
        "chalk": "^2.3.1",
        "commander": "^2.14.1",
        "dfdb": "^0.2.2",
        "es6-promise": "^4.2.4"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "mocha": "^5.0.1",
        "sprintf-js": "^1.1.1",
        "strip-ansi": "^4.0.0",
        "suppose": "^0.6.2"
    }
}